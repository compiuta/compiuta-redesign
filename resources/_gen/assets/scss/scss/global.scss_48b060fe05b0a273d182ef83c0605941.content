*{box-sizing:border-box}.body-tag{background-color:#000;color:#fff;transition:background-color,color .2s ease-in-out;padding:0;margin:0}.layout-styled{max-width:1200px;margin:0 auto}.divider{width:5px;height:100%;background-color:#fff}.projects-container{display:flex;flex-wrap:wrap;margin:0 -20px}.project-section{width:30%;padding:20px;flex:1 1 auto}.project{display:flex}.project-image-container{width:200px;height:200px;flex-shrink:0}.project-image-container.leaf{background:linear-gradient(#182b71,#192e76)}.project-image-container.light-bulb{background:linear-gradient(#f9dc00,#ffd74d)}.project-image-container.note{background:linear-gradient(#fa309c,#fc2899)}.project-image-container.dice{background:linear-gradient(#f9dc00,#ffd74d)}.project-image-container.books{background:linear-gradient(#fa309c,#fc2899)}.project-image-container.earth{background:linear-gradient(#182b71,#192e76)}.project-image{background-size:45%;background-repeat:no-repeat;background-position:50%;height:100%;width:100%}.leaf .project-image{background-image:url(/images/leaf.png)}.light-bulb .project-image{background-image:url(/images/light-bulb.png)}.note .project-image{background-image:url(/images/note.png)}.dice .project-image{background-image:url(/images/dice.png)}.books .project-image{background-image:url(/images/books.png)}.earth .project-image{background-image:url(/images/earth.png)}.theme-toggle{cursor:pointer;background-color:transparent;padding:0;border-width:0;border-style:none;border-color:transparent;border-image:initial;height:50px;width:50px;position:relative;overflow:hidden}.theme-toggle:focus{outline:none}.theme-icon{top:0;width:100%;position:absolute}.dark-icon{transition:right .2s ease-in-out;right:100%}.light-icon{transition:left .2s ease-in-out;top:0;left:0}.light-theme .dark-icon{right:0}.light-theme .light-icon{left:-100%}.light-theme.body-tag{color:#000;background-color:#fff}.light-theme .divider{background-color:#000}